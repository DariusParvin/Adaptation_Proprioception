function plot_correlation(data, xVar, yVar)


% Get x data
xVar = {xVar};
xData = data.(xVar{:});

% Get y data
yVar = {yVar};
yData = data.(yVar{:});


[rho,pval] = corrcoef(xData,yData)
r = rho(1,2)
p = pval(1,2)

xlabel('Dispersion (mm)')
ylabel('Asymptote (deg)')

str = sprintf('r = %.2f, p = %.2f', r, p)
title(str)

str3 = sprintf('Janet with pilot disp vs asym');
end